{
  "name": "@moor/web-sdk",
  "version": "1.0.0-rc1",
  "description": "Shared Web/API SDK for mooR clients",
  "type": "module",
  "main": "./dist/clients/web-sdk/src/index.js",
  "types": "./dist/clients/web-sdk/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/clients/web-sdk/src/index.d.ts",
      "import": "./dist/clients/web-sdk/src/index.js",
      "default": "./dist/clients/web-sdk/src/index.js"
    },
    "./*": {
      "types": "./dist/clients/web-sdk/src/*.d.ts",
      "import": "./dist/clients/web-sdk/src/*.js",
      "default": "./dist/clients/web-sdk/src/*.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://codeberg.org/timbran/moor.git",
    "directory": "clients/web-sdk"
  },
  "publishConfig": {
    "@moor:registry": "https://codeberg.org/api/packages/timbran/npm/"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsc -p tsconfig.build.json"
  },
  "dependencies": {
    "@moor/schema": "^1.0.0-rc1"
  },
  "devDependencies": {
    "typescript": "^5.8.2"
  },
  "license": "LGPL-3.0-or-later"
}
