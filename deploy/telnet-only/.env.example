# Telnet-Only Deployment Configuration
# Copy this file to .env and customize for your deployment

# Telnet server port (default: 8888)
TELNET_PORT=8888

# Database configuration
DATABASE_NAME=production.db

# Build profile (release for production, debug for testing)
BUILD_PROFILE=release

# Rust backtrace level for debugging (0, 1, or full)
RUST_BACKTRACE=1
